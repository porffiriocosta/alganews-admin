MMM yyyy', {\n          locale: ptBR,\n        });\n      },\n      formatter(data) {\n        return {\n          name: data.category === 'totalRevenues' ? 'Receitas' : 'Despesas',\n          value: (data.value as number).toLocaleString('pt-BR', {\n            currency: 'BRL',\n            style: 'currency',\n            maximumFractionDigits: 2,\n          }),\n        };\n      },\n    },\n    yAxis: false,\n    xAxis: {\n      label: {\n        formatter(item) {\n          return format(parseISO(item), 'MM/yyyy');\n        },\n      },\n    },\n    point: {\n      size: 5,\n      shape: 'circle',\n    },\n  };\n  return <Area {...config} />;\n}\n","import { Post, PostService } from 'danielbonifacio-sdk';\nimport { useCallback, useState } from 'react';\n\nexport default function useLatestPosts() {\n  const [posts, setPosts] = useState<Post.Paginated>();\n\n  const fetchPosts = useCallback(() => {\n    PostService.getAllPosts({\n      sort: ['createdAt', 'desc'],\n      page: 0,\n      size: 3,\n    }).then(setPosts);\n  }, []);\n\n  return {\n    posts: posts?.content,\n    fetchPosts,\n  };\n}\n","import { Card, Col, Row, Avatar } from 'antd';\nimport { useEffect } from 'react';\nimport useLatestPosts from '../../core/hooks/useLatestPosts';\n\nexport default function LatestPosts() {\n  const { fetchPosts, posts } = useLatestPosts();\n  useEffect(() => {\n    fetchPosts();\n  }, [fetchPosts]);\n\n  return (\n    <Row gutter={16}>\n      {posts?.map((post) => {\n        return (\n          <Col key={post.id} xs={24} md={8}>\n            <Card\n              cover={\n                <img\n                  alt={post.title}\n                  src={post.imageUrls.small}\n                  style={{\n                    height: 168,\n                    objectFit: 'cover',\n                  }}\n                />\n              }\n            >\n              <Card.Meta\n                avatar={\n                  <Avatar\n                    src={post.editor.avatarUr